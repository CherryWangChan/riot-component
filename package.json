{
  "name": "riot-component",
  "version": "0.0.3beta2",
  "description": "some components base on riot.js",
  "main": "./dist/riot-component.cjs.js",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015-rollup": "1.2.0",
    "bluebird": "^3.4.2",
    "browser-sync": "^2.14.0",
    "change-case": "^3.0.0",
    "cz-conventional-changelog": "^1.2.0",
    "del": "^2.2.2",
    "ghooks": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-if": "2.0.1",
    "gulp-inject-file": "^0.0.18",
    "gulp-inject-string": "^1.1.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-replace": "^0.5.4",
    "gulp-ruby-sass": "^2.1.0",
    "gulp-sequence": "^0.4.5",
    "gulp-typescript": "^3.1.3",
    "require-dir": "^0.3.1",
    "riot": "^2.4.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-commonjs": "^3.0.0",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.7.0",
    "rollup-plugin-riot": "^0.4.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-pluginutils": "^1.5.0",
    "typescript": "^2.0.10",
    "uglify-js": "^2.6.3"
  },
  "scripts": {
    "test": "npm run eslint && npm run karma",
    "eslint": "eslint src/*.js",
    "karma": "karma start test/karma.conf.js",
    "doc": "gulp doc",
    "build": "gulp build",
    "build:noCss": "gulp build:noCss --withCss false",
    "watch": "gulp watch",
    "server": "gulp server",
    "start": "gulp server:watch",
    "start:build": "gulp server:watch:build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsy0718/riot-component.git"
  },
  "keywords": [
    "riot",
    "riot-component",
    "component",
    "riot-calendar"
  ],
  "author": "fsy0718 <fsy0718@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fsy0718/riot-component/issues"
  },
  "homepage": "https://github.com/fsy0718/riot-component#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "./validate-commit-msg.js"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "build",
        "ci"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
